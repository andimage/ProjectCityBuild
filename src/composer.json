{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "0.9.0",
        "xpaw/php-minecraft-query": "^2.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.0.*",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "app/database/seeds",
            "app/database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Api\\": "app/api/",
            "App\\Console\\": "app/console/",
            "App\\Library\\": "app/library/",
            "App\\Support\\": "app/support/",
            "App\\Modules\\": "app/modules/",
            "Front\\": "front/",
            "Front\\Controllers\\": "front/controllers/",
            "Front\\Composers\\": "front/composers/",
            "Front\\Requests\\": "front/requests/",
            "Support\\": "support/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "app/tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "vendor-dir": "support/packages/vendor",
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
